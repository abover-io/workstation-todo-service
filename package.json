{
  "name": "sunday-todo-api",
  "version": "0.0.1",
  "description": "Sunday Todo API",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "server": "node ./dist/server.js",
    "transpile": "babel src -d dist -x .ts,.js,.json",
    "clean": "rimraf dist",
    "build": "npm-run-all clean transpile",
    "dev": "nodemon",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "test": "jest --coverage --detectOpenHandles --forceExit && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sundayexplore/todo-api.git"
  },
  "keywords": [
    "express",
    "mongodb",
    "mongoose",
    "typescript",
    "rest",
    "api",
    "todo",
    "todolist",
    "opensource"
  ],
  "author": "Adam Rafiandri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sundayexplore/todo-api/issues"
  },
  "homepage": "https://github.com/sundayexplore/todo-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.16.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.10.2",
    "helmet": "^4.6.0",
    "http-errors": "^1.7.3",
    "immutability-helper": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "redis": "^3.1.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "coveralls": "^3.0.11",
    "dotenv": "^10.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  }
}
